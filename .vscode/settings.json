{
  "files.associations": {
    "*.md": "markdown",
    "*.yml": "yaml"
  },
  "markdown-preview-enhanced.previewTheme": "github-light.css",
  "markdown-preview-enhanced.liveUpdate": true,
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "yaml.validate": true,
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": "/*.github/workflows/*.yml"
  },
  "terminal.integrated.defaultProfile.windows": "Command Prompt",

  // Rechtschreibprüfung
  "cSpell.language": "en,de,de-DE",
  "cSpell.dictionaries": [
    "en_US",
    "de-de"
  ],
  "cSpell.words": [
    "Biegemoment",
    "Creo",
    "Festigkeitsberechnung",
    "Gebhardt",
    "glightbox",
    "Hookesche",
    "Hookeschen",
    "Hookesches",
    "kontinuumsmechanische",
    "kragbalken",
    "Kragbalken",
    "Kragbalkens",
    "Mises",
    "Modellierungsaufgaben",
    "Poissonzahl",
    "Querkontraktionszahl",
    "Querschnittsänderung",
    "Querschnittsfläche",
    "Querschnittsmaß",
    "Selektionsfunktionen",
    "STRG",
    "Tangentenwinkel",
    "Vergleichsspanunng",
    "Verschiebungsrandbedingung"
  ]
}